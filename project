#include<stdio.h>
#include<unistd.h>
#include<stdlib.h>
int main()
{
        int i;
        while(1)
        {
                for(i=0;i<64;i++)
                {
                        int j;
                        system("clear");
                        printf("\n\n\n\n\n\n\n\n\n\n");
                        for(j=0;j<i;j++)
                                printf("_");
                //      puts("hai");
                //printf("\n\n\n\n\n\n\n\n\n");
                        usleep(80000);
                }
        }
}
